{
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "./dist",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/vercel-proxy.js"
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}