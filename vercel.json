{
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "./dist",
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://admin.momtabare.com/api/:path*"
    },
    {
      "source": "/sanctum/:path*",
      "destination": "https://admin.momtabare.com/sanctum/:path*"
    }
  ]
}
